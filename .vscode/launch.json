{
  "version": "0.2.0",
"configurations": [
    {
        "name": "run-print-full-screen",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": [
            "fiori",
            "run"
        ],
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npx",
        "args": [
            "--open",
            "test/flpSandbox.html"
        ],
        "windows": {
            "runtimeExecutable": "npx.cmd"
        },
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "outputCapture": "std",
        "env": {
            "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/print-full-screen\"}",
            "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false"
        }
    }
]
}